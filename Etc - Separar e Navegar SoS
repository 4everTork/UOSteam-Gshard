; If using Britannian Ship, stand on deck behind the hold
; If using Tokuno Ship, stand on the hold

; SCRIPT SETUP
set %shardType Odyssey ; can be OSI, Pandora, POL, RunUO, Alexandria, Odessa, Odyssey
if #SHARD = Ultima , #spc , Forever || #shard = Pandora
 set %shardType Pandora
if #Shard = Odyssey , #spc , II , #spc , - , #spc , 1
 set %ShardType Odyssey
gosub setCurPath #curpath , _scripts\SOS master\

set %Say_Whisper_Yell 1 ; 1=say, 3=whisper, 4=yell Some shards can't deal with whispering commands to tillerman from the front of the boat, so must set to say (1)
; END SETUP

; COLORS SETUP
set %MapWaterColor     $FF5555
set %MapLandColor      $66ff66
set %MapMountainColor  $aaaaaa ; comment out to prevent display (HIGH DETAIL map only)
set %MapRegionColor    $8888ff
set %MapRegionTColor   aqua
set %MapServerLColor   $ff88ff ; comment out to prevent display
set %MapSOSMarkerColor $FFFFFF
; END SETUP

sub setCurPath
 if %0 > 1
  {
  for %_ 2 %0
   {
   set %1 %1 , #spc , % . %_
   }
  }
 set %curpath %1
return


set #lpc 100
gosub KalOCRInit
set %_SOSData _
set %_delay4actions 0
gosub setTypeInfoFor , %shardType
gosub showsosmastermenu
menu set status drawing world map
;call %curpath , worldmap.txt
gosub WorldMap
;call %curpath , sosregions.txt
;if %_SOSRegion_Cnt = n/a
;	gosub createSOSRegionsFile
sub defineSOSRegion
 set %_sosregion_cnt %_sosregion_cnt + 1
 set %_sosregion_x1_ . %1 %2
 set %_sosregion_y1_ . %1 %3
 set %_sosregion_x2_ . %1 %4
 set %_sosregion_y2_ . %1 %5
 set %_sosregion_col_ . %1 %6
return
set %_sosregion_cnt 0
;gosub defineSOSRegion 1 0 0 4798 4059 lime
gosub defineSOSRegion 1 2248 1137 3420 1801 black
gosub defineSOSRegion 2 3557 53 3792 940 red
gosub defineSOSRegion 3 3908 1739 4798 2859 lime
gosub defineSOSRegion 4 2332 2546 3245 3095 yellow
gosub defineSOSRegion 5 3341 3153 4235 3522 blue
gosub defineSOSRegion 6 1606 3712 2034 4059 fuchsia
gosub defineSOSRegion 7 2657 3800 4022 3957 aqua
gosub defineSOSRegion 8 0 130 91 1524 white
gosub defineSOSRegion 9 0 1559 683 2019 gray
gosub defineSOSRegion 10 0 2084 308 3989 green

;for %0 1 %_sosregion_cnt
;   {
;   set %1 %_sosregion_x1_ . %0 + ( %_sosregion_x2_ . %0 - %_sosregion_x1_ . %0 )
;   set %2 %_sosregion_y1_ . %0 + ( %_sosregion_y2_ . %0 - %_sosregion_y1_ . %0 )
;   set %_sosregion_x1_ . %0 %1
;   set %_sosregion_y1_ . %0 %2
;   set %_sosregion_x2_ . %0 %1
;   set %_sosregion_y2_ . %0 %2
;   }

;call %curpath , sosdata.txt
;call %curpath , _test_pin_file.txt
gosub redrawSOSRegions
gosub redrawSOSOnMap
menu set status ready!

; close open SOS gumps

while #contsize = %sosSize
 {
 set %1 #contposx + 50
 set %2 #contposy + 50
 click %1 %2 r f dmc
 wait 10
 }

gosub MainMenu
halt

sub MainMenu
MainMenu:
 menu Font Transparent #true
 menu Font Align Right
 menu Font Name MS Sans Serif
 menu Font Size 8
 menu Font Style
 menu Font Color WindowText
 menu Font BGColor BtnFace
 menu Font Align Left
 menu Text Menu_Label 328 8 Main Menu
 ;menu Text EUOLabel1 416 8 Script Setup
 menu Button loadSOSInfo 328 24 83 25 Load SOS Info
 menu Button clearSOSInfo 328 52 83 25 Clear SOS Info
 menu Button MoveSOSTo 328 80 83 25 Move SOS To..
 menu Button DragMarkedSOS 328 108 83 25 Drag Marked
 menu Button deleteMarked 328 136 83 25 Delete Marked
 menu Button SailToMarked 328 164 83 25 Sail To Marked
 ;menu Check SetTypeInfoForOSI 416 24 97 17 #false OSI Shard
 ;menu Check SetTypeInfoForPOL 416 40 97 17 #false POL Shard
 ;menu Set SetTypeInfo , %shardType #true
 gosub WaitForMenuEvent
 menu Delete Menu_Label
 ;menu Delete EUOLabel1
 menu Delete loadSOSInfo
 menu Delete clearSOSInfo
 menu Delete makeMapFile
 menu Delete MoveSOSTo
 menu Delete DragMarkedSOS
 menu Delete deleteMarked
 menu Delete SailToMarked
 ;menu Delete SetTypeInfoForOSI
 ;menu Delete SetTypeInfoForPOL
 if #menubutton = loadSOSInfo
	gosub loadSOSInfo
 if #menubutton = clearSOSInfo
	gosub clearSOSInfo
 if #menubutton = makeMapFile
	gosub makeMapFile
 if #menubutton = MoveSOSTo
	gosub MoveSOSTo
 if #menubutton = DragMarkedSOS
	gosub DragMarkedSOS
 if #menubutton = deleteMarked
	gosub deleteMarked
 if #menubutton = SailToMarked
	gosub SailToMarked
; if #menubutton = SetTypeInfoForOSI
;	 gosub SetTypeInfoForOSI
; if #menubutton = SetTypeInfoForPOL
;	 gosub SetTypeInfoForOSI
 if #menubutton <> closed
	goto MainMenu
return

sub setTypeInfoForAlexandria
 set %regionDrawRoutine Alexandria
 set %shardType Alexandria
 set %mibType HTD
 set %sosType RVH
 set %sosSize 254_400
 set %holdNorthType UVX
 set %holdWestType ZUX
 set %holdEastType ZNX
 set %holdSouthType LWX
 set %holdType %holdNorthType , _ , %holdWestType , _ , %holdEastType , _ , %holdSouthType
return

sub setTypeInfoForRunUO
 set %regionDrawRoutine Normal
 set %shardType RunUO
 set %mibType HTD
 set %sosType RVH
 set %sosSize 254_400
 set %holdNorthType UVX
 set %holdWestType ZUX
 set %holdEastType ZNX
 set %holdSouthType LWX
 set %holdType %holdNorthType , _ , %holdWestType , _ , %holdEastType , _ , %holdSouthType
return

sub setTypeInfoForOSI
 set %regionDrawRoutine Normal
 set %shardType OSI
 set %mibType HTD_AWJC
 set %sosType UVH
 set %sosSize 254_400
 ;hold types for classic, Tokuno, Britannian ships
 set %holdNorthType UVX_XZCC_DAJB
 set %holdWestType ZUX_CVCC_PEJB
 set %holdEastType ZNX_EFDC_DTIB
 set %holdSouthType LWX_VPCC_PXIB
 set %holdType %holdNorthType , _ , %holdWestType , _ , %holdEastType , _ , %holdSouthType
return

