sub LoadSOSInfo
 ; BUST OPEN MIBS
 finditem %mibType c
 if #findkind <> -1
  {
  menu hide
  display yesno Would you like the script to open MIBs?
  menu show
  set #menubutton n/a
  if #dispres = yes
   {
   for !mibIndex #findcnt 1
    {
    set #findindex !mibIndex
    if #findcol = 0
     {
     set #lobjectid #findid
     event macro 17 0
     wait 20
     }
    }
   }
  if #targcurs = 1
  key esc
  }

 finditem %sosType c
 if #findkind = -1
	{
	menu set status no sos found to load!
	return
	}

 menu Font Transparent #true
 menu Font Align Right
 menu Font Name MS Sans Serif
 menu Font Size 8
 menu Font Style
 menu Font Color WindowText
 menu Font Align Left
 menu Text EUOLabel1 328 8 The script is "scanning" coordinates
 menu Text EUOLabel2 328 20 from SOS gumps using a process
 menu Text EUOLabel3 328 32 sometimes referred to as pixel color
 menu Text EUOLabel4 328 44 testing. IE: now would be a really
 menu Text EUOLabel5 328 56 bad time to minimize UO or cause
 menu Text EUOLabel6 328 68 other windows to cover up the area
 menu Text EUOLabel7 328 80 of the UO window the coordinates
 menu Text EUOLabel8 328 92 are being scanned from.

; load un-listed SOS
 set !lpc #lpc
 set #lpc 10000
 for %_index #findcnt 1
  {
  finditem %sosType %_index c
  if #findkind <> -1 && #findid notin %_SOSData
   {
   gosub scanSOS #findid
   gosub LoadSOSData %_sosworldx %_sosworldy #findid
   }
  }
 set #lpc 100

; remove SOS that can't be found
 set %_temp %_SOSData
 set %_SOSData _
 str len %_temp
 set #strres #strres / 18
 set %_REdrawFlag #false
 for %_loop #strres 1
  {
  str pos %_temp _
  if #strres = 0 || #strres > 8
   set #strres 8
  else
   set #strres #strres - 1
  str mid %_temp 1 #strres
  finditem #strres
  if #findkind = -1
   set %_redrawFlag #true
  if #findkind <> -1
   {
   str mid %_temp 1 18
   set %_SOSData #strres , %_SOSData
   }
  str del %_temp 1 18
  set %_temp #strres
  }
 gosub redrawSOSRegions
 if %_redrawFlag = #true
  gosub redrawSOSOnMap
 gosub markClosestSOSLocation
 menu set status SOS Info loaded
 menu Delete Menu_BKGND
 menu Delete EUOLabel1
 menu Delete EUOLabel2
 menu Delete EUOLabel3
 menu Delete EUOLabel4
 menu Delete EUOLabel5
 menu Delete EUOLabel6
 menu Delete EUOLabel7
 menu Delete EUOLabel8
 menu Delete EUOLabel9
return

sub ClearSOSInfo
 set #menubutton n/a
 menu set status clearing SOS info
 str len %_SOSDATA
 set %_looper #strres / 18
 for %_eetmp 1 %_looper
  {
  str left %_SOSDATA 7
  menu delete #strres
  str del %_SOSDATA 1 18
  set %_SOSDATA #strres
  }
 gosub redrawSOSOnMap
 gosub markClosestSOSLocation
 menu set status SOS info cleared
return

sub MoveSOSTo
MoveSOSto:
 menu Font Transparent #true
 menu Font Align Right
 menu Font Name MS Sans Serif
 menu Font Size 8
 menu Font Style
 menu Font Color WindowText
 menu Font Align Left
 menu Font BGColor BtnFace
 menu Text EUOLabel1 328 8 Select the sos region number(s) to
 menu Text EUOLabel2 328 20 grab SOS from then click the "ok"
 menu Text EUOLabel3 328 32 button. You then need to place your
 menu Text EUOLabel4 328 44 cursor over the container you want
 menu Text EUOLabel5 328 56 to move the SOS into and press the
 menu Text EUOLabel6 328 68 spacebar. The script will move * , all*
 menu Text EUOLabel7 328 80 sos from the region(s) to a container.
 menu Button EUOButton1 332 256 43 17 ok
 menu Button EUOButton2 380 256 75 17 cancel
 menu Button EUOButton3 332 232 51 21 select all
 menu Button EUOButton4 388 232 67 21 none
 for %_ 1 %_SOSRegion_cnt
  {
  set %_y %_ * 12 + 88
  menu Check SOSRegion , %_ 354 %_Y 97 17 #false SOS region # , %_
  }
MoveSOSTo2:
 gosub WaitForMenuEvent
 if #menubutton = EUOButton3
  {
  for %_ 1 %_SOSRegion_cnt
   {
   menu set SOSRegion , %_ #true
   }
  goto MoveSOSTo2
  }
 if #menubutton = EUOButton4
  {
  for %_ 1 %_SOSRegion_cnt
   {
   menu set SOSRegion , %_ #false
   }
  goto MoveSOSTo2
  }
 menu Delete Menu_BKGND
 menu Delete EUOLabel1
 menu Delete EUOLabel2
 menu Delete EUOLabel3
 menu Delete EUOLabel4
 menu Delete EUOLabel5
 menu Delete EUOLabel6
 menu Delete EUOLabel7
 menu Delete EUOButton1
 menu Delete EUOButton2
 menu Delete EUOButton3
 menu Delete EUOButton4
 set %_region _
 set %sosregion ; (yeuo)
 for %_ 1 %_SOSRegion_cnt
  {
  menu get SOSRegion , %_
  if #menures = #true 2
   set %sosregion %sosregion , #spc , %_ ; (yeuo)
  set %_region %_region , %_ , _
  menu Delete SOSRegion , %_
  }
 if #menubutton = EUOButton2
  return
 str len %_SOSData
 if #strres = 0
  {
  menu set status SOS cant be moved as no info is loaded
  return
  }
 menu set status position cursor over bag and press spacebar
 for %_ 1 1
  {
  wait 1
  set %_ 0
  onhotkey space
  set %_ 1
  }
 set %_xpos #cursorx
 set %_ypos #cursory
 menu set status moving SOS from region(s) %sosregion
 str len %_SOSData
 set %_temp %_SOSData
 set #strres #strres / 18
 for %_loop #strres 1
  {
  str mid %_temp 17 2
  set #strres #strres + 0
  if _ , #strres , _ in %_region
   {
   str pos %_temp _
   if #strres = 0 || #strres > 8
    str mid %_temp 1 8
   else
    {
    set #strres #strres - 1
    str mid %_temp 1 #strres
    }
   finditem #strres
   if #findkind <> -1
    {
    if #lliftedkind = 0
     {
     event drag #findid
     wait 5
     }
    click %_xpos %_ypos p f dmc
    wait 20
    }
   }
  str del %_temp 1 18
  set %_temp #strres
  }
 menu set status finished moving SOS to container
return

sub waitForMenuEvent
waitForMenuEvent:
 set #menubutton n/a
 for !_ 1 1
  {
  if #charposx / 16 <> %_charmapposx || #charposy / 16 <> %_charmapposy
   {
   gosub redrawPlayerPosition
   gosub markClosestSOSLocation
   }
  if #menubutton = n/a
   {
   set !_ 0
   wait 1
   }
  }
 if #menubutton = saveSOSData
  {
  gosub saveSOSData
  goto waitForMenuEvent
  }
return

sub SailToMarked
 menu Font Transparent #true
 menu Font Align Right
 menu Font Name MS Sans Serif
 menu Font Size 8
 menu Font Style
 menu Font Color WindowText
 menu Font Align Left
 menu Text EUOLabel1 328 8 The script is sailing to the "marked"
 menu Text EUOLabel2 328 20 SOS location shown on the map. If
 menu Text EUOLabel3 328 32 any obstacles are encountered
 menu Text EUOLabel4 328 44 the script will stop and return you
 menu Text EUOLabel5 328 56 back to the main menu... you can
 menu Text EUOLabel6 328 68 also click the "cancel" button to
 menu Text EUOLabel7 328 80 exit this sailing routine early.
 menu Button EUOButton2 380 256 75 17 cancel

 menu set status sailing to marked SOS
 if %_CLOSESTSOSX = 0 && %_CLOSESTSOSY = 0
  {
  menu set status No SOS loaded, cancelling sail
  goto sailToMarkedEnd
  }
 gosub _findShipsHold
 gosub _getShipInfo
 set %1 %_closestSOSx
 set %2 %_closestSOSy - 2
 set %_xdist %1 - %_shipsX abs
 set %_ydist %2 - %_shipsY abs
 ; turn the boat
 if %_xdist > %_ydist
  {
  if %_shipsx > %1
   set %_destDir 7
  if %_shipsx = %1
   {
   if %_shipsy > %2
    set %_destDir 1
   if %_shipsy = %2
    goto sailToMarkedEnd
   if %_shipsy < %2
    set %_destDir 5
   }
  if %_shipsx < %1
   set %_destDir 3
  }
 if %_xdist = %_ydist
  {
  if %_shipsx > %1
   set %_destDir 7
  if %_shipsx = %1
   goto sailToMarkedEnd
  if %_shipsx < %1
   set %_destDir 3
  }
 if %_xdist < %_ydist
  {
  if %_shipsy > %2
   set %_destDir 1 ; (Garath) was set to 2
  if %_shipsy = %2
   {
   if %_shipsx > %1
    set %_destDir 7
   if %_shipsx = %1
    goto sailToMarkedEnd
   if %_shipsx < %1
    set %_destDir 3
   }
  if %_shipsy < %2
   set %_destDir 5
  }
 set %_ ( %_destDir - %_shipsDir + 9 ) % 8
 if %_ = 3 || %_ = 5 || %_ = 7
  {
  if %_ = 3
   event macro %Say_Whisper_Yell 0 turn right
  if %_ = 5
   event macro %Say_Whisper_Yell 0 come about
  if %_ = 7
   event macro %Say_Whisper_Yell 0 turn left
  wait 1s
  set %_shipsDir %_destDir
  }
 ; set boat watches
 if %1 <= %_shipsX
  set %_watchX <=
 if %1 > %_shipsX
  set %_watchX >=
 if %2 <= %_shipsY
  set %_watchY <=
 if %2 > %_shipsY
  set %_watchY >=
 ; sail the "diagonal"
 if %1 < %_shipsX && %2 < %_shipsY
  set %_destDir 0
 if %1 = %_shipsX && %2 < %_shipsY
  set %_destDir 1
 if %1 > %_shipsX && %2 < %_shipsY
  set %_destDir 2
 if %1 > %_shipsX && %2 = %_shipsY
  set %_destDir 3
 if %1 > %_shipsX && %2 > %_shipsY
  set %_destDir 4
 if %1 = %_shipsX && %2 > %_shipsY
  set %_destDir 5
 if %1 < %_shipsX && %2 > %_shipsY
  set %_destDir 6
 if %1 < %_shipsX && %2 = %_shipsY
  set %_destDir 7
 if %1 = %_shipsX && %2 = %_shipsY
  goto sailToMarkedEnd
 if %_destDir in 0246
  {
  set %_ ( %_destDir - %_shipsDir + 9 ) % 8
  if %_ = 0
   event macro %Say_Whisper_Yell 0 forward left
  if %_ = 2
   event macro %Say_Whisper_Yell 0 forward right
  set %_shipsTimer #scnt2 + 30
  for %_ 1 1
   {
   if #menubutton = EUOButton2
    {
    event macro %Say_Whisper_Yell 0 stop
    menu set status Ready!
    goto sailToMarkedEnd
    }
   if #charposx / 16 <> %_charmapposx || #charposy / 16 <> %_charmapposy
    {
    gosub redrawPlayerPosition
    }
   gosub _getShipInfo
   if %_shipsX , _ , %_shipsY <> %_shipsPos
    {
    set %_shipsPos %_shipsX , _ , %_shipsY
    set %_shipsTimer #scnt2 + 30
    }
   if #scnt2 >= %_shipsTimer
    {
    event macro %Say_Whisper_Yell 0 stop
    menu set status unable to reach marked SOS
    goto sailToMarkedEnd
    }
   if ! ( %_shipsX %_watchX %1 || %_shipsY %_watchY %2 )
    {
    wait 1
    set %_ 0
    }
   }
  }
 ; sail the straight
 event macro %Say_Whisper_Yell 0 forward
 set %_shipsTimer #scnt2 + 30
 for %_ 1 1
  {
  if #menubutton = EUOButton2
   {
   event macro %Say_Whisper_Yell 0 stop
   menu set status Ready!
   goto sailToMarkedEnd
   }
  if #charposx / 16 <> %_charmapposx || #charposy / 16 <> %_charmapposy
   gosub redrawPlayerPosition
  gosub _getShipInfo
  if %_shipsX , _ , %_shipsY <> %_shipsPos
   {
   set %_shipsPos %_shipsX , _ , %_shipsY
   set %_shipsTimer #scnt2 + 30
   }
  if #scnt2 >= %_shipsTimer
   {
   event macro %Say_Whisper_Yell 0 stop
   menu set status unable to reach marked SOS
   goto sailToMarkedEnd
   }
  if ! ( %_shipsX %_watchX %1 && %_shipsY %_watchY %2 )
   {
   wait 1
   set %_ 0
   }
  }
 event macro %Say_Whisper_Yell 0 stop
 ; turn boat west
 wait 1s
 set %_ ( 7 - %_shipsDir + 9 ) % 8
 if %_ in 357
  {
  if %_ = 3
   event macro %Say_Whisper_Yell 0 turn right
  if %_ = 5
   event macro %Say_Whisper_Yell 0 come about
  if %_ = 7
   event macro %Say_Whisper_Yell 0 turn left
  wait 1s
  set %_shipsDir %_destDir
  }
 menu set status sailed to marked SOS destination
sailToMarkedEnd:
 menu Delete EUOLabel1
 menu Delete EUOLabel2
 menu Delete EUOLabel3
 menu Delete EUOLabel4
 menu Delete EUOLabel5
 menu Delete EUOLabel6
 menu Delete EUOLabel7
 menu Delete EUOButton2
return

sub _getShipInfo
 finditem %_shipsHold
 if #findtype in %holdNorthType
  {
  set %_shipsX #findx
  set %_shipsY #findy + 4
  set %_shipsDir 1
  }
 if #findtype in %holdEastType
  {
  set %_shipsX #findx - 4
  set %_shipsY #findy
  set %_shipsDir 3
  }
 if #findtype in %holdSouthType
  {
  set %_shipsX #findx
  set %_shipsY #findy - 4
  set %_shipsDir 5
  }
 if #findtype in %holdWestType
  {
  set %_shipsX #findx + 4
  set %_shipsY #findy
  set %_shipsDir 7
  }
return

sub _findShipsHold
 namespace push
 namespace local _temp
 set !_shipsHold n/a
 set !_holdsDist 999
 finditem %holdType g
 for !_ #findcnt 1
  {
  finditem %holdType !_ g
  set !_xdist #findx - #charposx abs
  set !_ydist #findy - #charposy abs
  set !_dist !_ydist
  if !_xdist > !_ydist
  set !_dist !_xdist
  if !_dist < !_holdsDist
   {
   if #findtype in %holdNorthType && #charposy > #findy && !_xdist < 2
    {
    set !_holdsDist !_dist
    set !_shipsHold #findid
    }
   if #findtype in %holdEastType && #charposx < #findx && !_ydist < 2
    {
    set !_holdsDist !_dist
    set !_shipsHold #findid
    }
   if #findtype in %holdSouthType && #charposy < #findy && !_xdist < 2
    {
    set !_holdsDist !_dist
    set !_shipsHold #findid
    }
   if #findtype in %holdWestType && #charposx > #findx && !_ydist < 2
    {
    set !_holdsDist !_dist
    set !_shipsHold #findid
    }
   }
  }
 set #result !_shipsHold
 namespace clear
 namespace pop
 set %_shipsHold #result
return #result

sub DeleteMarked
 str len %_closestSOSID
 str del ________ 1 #strres
 set %1 %_closestSOSID , #strres
 str pos %_SOSData %1
 if #strres = 0
  {
  menu set status can not remove marked SOS
  }
 if #strres > 0
  {
  menu set status deleting marked SOS
  str del %_SOSData #strres 18
  set %_SOSData #strres
  gosub redrawSOSOnMap
  menu set status updating SOS marker
  gosub markClosestSOSLocation
  menu set status marked SOS removed
  }
return

sub DragMarkedSOS
 set #menubutton n/a
 if #lliftedkind = 1
  {
  menu set status something "floating" on cursor
  return
  }
 finditem %_closestSOSID
 if #findkind = -1
  menu set status Marked SOS not found
 else
  {
  menu set status Marked SOS "floating" on cursor
  event drag #findid
  }
return

sub markClosestSOSLocation
 set %_d 9999
 set %_x 0
 set %_y 0
 set %_id x

 str len %_SOSData
 set %_temp %_SOSData
 set #strres #strres / 18
 for %_loop #strres 1
  {
  str mid %_temp 9 4
  set %2 #strres + 0
  str mid %_temp 13 4
  set %3 #strres + 0

  set %_1 %2 - #charposx abs
  set %_2 %3 - #charposy abs
  if %_1 > %_2
  set %_ %_1
  else
  set %_ %_2

  if %_ < %_d
   {
   str pos %_temp _
   if #strres = 0 || #strres > 8
    str mid %_temp 1 8
   else
    {
    set #strres #strres - 1
    str mid %_temp 1 #strres
    }
   set %_id #strres
   set %_x %2
   set %_y %3
   set %_d %_
   }
  str del %_temp 1 18
  set %_temp #strres
  }

 if %_id <> %_closestSOSID
  {
  set %_closestSOSx %_x
  set %_closestSOSy %_y
  ;set %_closestSOSDist %_d
  set %_closestSOSID %_id
  set %_1 %_x
  set %_2 %_y
  menu delete SOSMarker
  gosub drawmarker SOSMarker %_x %_y %MapSOSMarkerColor
  set %_x %_1
  set %_y %_2
  ;		set %1 %_x / 16 - 4
  ;		set %2 %_y / 16 - 4
  ;		menu delete SOSMarker
  ;		menu shape SOSMarker %1 %2 10 10 2 7 1 $0000ff 2 0

  set %1 20 ;%1 + 5 ; draw region # with "border"
  set %2 10 ;%2 + 4
  set %3 %1 + 2
  set %4 %2 + 2
  ;set %_id URSOSID
  menu font color white
  menu Font Name MS Sans Serif
  menu font BGColor %MapWaterColor
  for %5 %1 %3
   {
   for %6 %2 %4
    {
    menu delete SOSLabel
    menu text SOSLabel %5 %6 %_id , $x %_x , $y %_y
    }
   }
  set %1 %1 + 1
  set %2 %2 + 1
  menu font color white
  menu font BGColor %MapWaterColor
  menu delete SOSLabel
  menu text SOSLabel %1 %2 %_id , $x %_x , $y %_y
  menu Font BGColor BtnFace
  }

return

sub drawMarker
 ;display drawMarker %1 %2 %3
 menu delete %1 , north
 menu delete %1 , east
 menu delete %1 , west
 menu delete %1 , south
 str len %_SOSData
 if #strres = 0 || #strres = 1
  return
 set %_xpos %2 / 16
 set %_ypos %3 / 16
 set %_x %_xpos - 4
 set %_y %_ypos - 4
 menu shape %1 %_x %_y 10 10 2 7 1 %4 2 0
 set %_x %_xpos - 6
 menu shape %1 , north 2 %_ypos %_x 1 3 7 1 %4 2 0
 set %_x %_xpos + 6
 set %_l 318 - %_x
 menu shape %1 , east %_x %_ypos %_l 1 3 7 1 %4 2 0
 set %_l %_ypos - 6
 menu shape %1 , west %_xpos 2 1 %_l 3 7 1 %4 2 0
 set %_y %_ypos + 6
 set %_l 254 - %_y
 menu shape %1 , south %_xpos %_y 1 %_l 3 7 1 %4 2 0
return

sub scanSOS
 namespace push
 namespace local _temp
 if #scnt2 < %_delay4actions
  {
  set !_ ( %_delay4actions - #scnt2 ) * 2
  wait !_
  }
 set #lobjectid %1
 event macro 17 0
 for !_ 60 1
  {
  if #contsize <> %sosSize
   wait 1
  if #contsize = %sosSize
   set %_ 1
  }
 if #contsize <> %sosSize
  {
  namespace clear
  namespace pop
  return #false
  }
 set %_delay4actions #scnt2 + 9
 menu set status scanning unlisted SOS ( , %_index , )
 gosub contpos 0 0 50 50
 wait 7
 gosub getsosinfo 0 0
 click 50 50 r f dmc
 set %_sosCoords !sos_coords
 set %_sosWorldX !sos_worldx
 set %_sosWorldY !sos_worldy
 namespace clear
 namespace pop
return #true

sub saveSOSData
 set #menubutton n/a
 menu set status saving sos info...
 str len %_SOSData
 if #strres = 0
  return

 gosub open 1 %curpath , sosdata.txt
 gosub wipe 1

 set %_temp set , #spc , % , _SOSData , #spc , %_SOSData
 for %_ 1 1
  {
  str len %_temp
  if #strres <= 80
   {
   gosub write 1 %_temp
   set %_ 1
   if #true 0
   }
  else
   {
   str left %_temp 80
   gosub write 1 #strres
   str del %_temp 1 80
   set %_temp + , #strres
   set %_ 0
   }
  }
 gosub dump 1
 menu set status sos info saved for next launch!
return

sub closed
 set #menubutton n/a
 halt
return

sub redrawPlayerPosition
 set %_charmapposx #charposx / 16
 set %_charmapposy #charposy / 16
 set %_1 %_charmapposx - 2
 set %_2 %_charmapposy - 2
 menu delete playerPosition
 menu shape playerPosition %_1 %_2 6 6 2 7 1 $000000 7 $0088ff
return

sub loadSOSData
 if %_SOSData = n/a
 set %_SOSData _
 set %4 0
 for %_ 1 %_sosregion_cnt
  {
  if %_sosregion_x1_ . %_ <= %1 && %_sosregion_y1_ . %_ <= %2 && %_sosregion_x2_ . %_ >= %1 && %_sosregion_y2_ . %_ >= %2
  set %4 %_
  }
 if %4 = 0
  {
  set !_regionD 9999
  for %i 1 %_sosregion_cnt
   {
   set !_xd abs ( %1 - ( %_sosregion_x1_ . %i + %_sosregion_x2_ . %i - %_sosregion_x1_ . %i ) )
   set !_yd abs ( %2 - ( %_sosregion_y1_ . %i + %_sosregion_y2_ . %i - %_sosregion_y1_ . %i ) )
   set !_dist !_xd
   if !_yd > !_xd
    set !_dist !_yd
   if !_dist < !_regionD
    {
    set !_regionD !_dist
    set %4 %i
    }
   }
; resize region to fit...
  if %4 <> 0
   {
   if %_sosregion_x1_ . %4 > %1
    set %_sosregion_x1_ . %4 %1
   if %_sosregion_y1_ . %4 > %2
    set %_sosregion_y1_ . %4 %2
   if %_sosregion_x2_ . %4 < %1
    set %_sosregion_x2_ . %4 %1
   if %_sosregion_y2_ . %4 < %2
    set %_sosregion_y2_ . %4 %2
   ;gosub redrawSOSRegions
   }
  ; finding a region for SOS failed!!!
  else
   {
   gosub _getClosestRegion
   }
  }
 gosub _loadSOSData
 set %1 %1 / 16 - 2
 set %2 %2 / 16 - 2
 ;	pause
 menu shape %3 %1 %2 6 6 2 7 1 $000000 7 %_sosregion_col_ . %4
return

sub _loadSOSData ; <x> <y> <id> <region>
 str len %4
 str del 00 1 #strres
 set %_SOSData #strres , %4 , %_SOSData
 str len %2
 str del 0000 1 #strres
 set %_SOSData #strres , %2 , %_SOSData
 str len %1
 str del 0000 1 #strres
 set %_SOSData #strres , %1 , %_SOSData
 str len %3
 str del ________ 1 #strres
 set %_SOSData %3 , #strres , %_SOSData
return

sub _getClosestRegion
 menu Font Transparent #true
 menu Font Name MS Sans Serif
 menu Font Size 8
 menu Font Style
 menu Font Color WindowText
 menu Font Align Left
 menu Font Transparent #true
 menu Text Label 328 116 Location shown on map via the
 menu Text Label 328 128 crosshairs identifies a location
 menu Text Label 328 140 outside any of the defined regions.
 menu Text Label 328 152 You need to select the number for
 menu Text Label 328 164 the region this SOS location
 menu Text Label 328 176 belongs in so that the region
 menu Text Label 328 188 definition file can be updated.
 menu Font BGColor Window
 menu Combo Create LabelR 328 224 165
 menu Font BGColor BtnFace
 menu Button Label 328 248 31 17 ok
 menu Font Style b
 menu Text Label 328 208 Select region then click ok
 for !_ 1 %_sosregion_cnt
  {
  menu Combo Add LabelR sos region !_
  }
 menu delete sosmarker

 set %_xpos %1 / 16
 set %_ypos %2 / 16
 menu shape label %_xpos 2 1 252 3 7 1 $ffffff 7 $ffffff
 menu shape label 2 %_ypos 315 1 3 7 1 $ffffff 7 $ffffff
 set %_xpos %_xpos - 3
 set %_ypos %_ypos - 3
 menu shape label %_xpos %_ypos 6 6 2 7 1 $ffffff 7 $ffffff
 set #menubutton n/a
 while #menubutton = n/a
  {
  wait 1
  menu get labelR
  if #menubutton <> n/a && #menures = 0
   set #menubutton n/a
  }
 menu delete label
 menu delete LabelR

; enlarge region so that this location is bounded by it

 if %1 < %_sosregion_x1_ . #menures
  set %_sosregion_x1_ . #menures %1
 if %2 < %_sosregion_y1_ . #menures
  set %_sosregion_y1_ . #menures %2
 if %1 > %_sosregion_x2_ . #menures
  set %_sosregion_x2_ . #menures %1
 if %2 > %_sosregion_y2_ . #menures
  set %_sosregion_y2_ . #menures %2

 set %_1 %1
 set %_2 %2
 set %_3 %3
 set %_4 #menures
 gosub redrawSOSRegions
 gosub writeSOSRegionsFile
 set %1 %_1
 set %2 %_2
 set %3 %_3
 set %4 %_4
return

sub redrawSOSOnMap
 menu delete %_CLOSESTSOSID
 menu delete CLOSESTSOSTARGET
 str len %_SOSData
 if #strres = 0 || #strres = 1
  return
 set %_temp %_SOSData
 set #strres #strres / 18
 for %_loop #strres 1
  {
  str mid %_temp 9 4
  set %2 #strres / 16 - 2
  str mid %_temp 13 4
  set %3 #strres / 16 - 2
  str mid %_temp 17 2
  set %4 #strres + 0
  menu shape CLOSESTSOSTARGET %2 %3 6 6 2 7 1 $000000 7 %_sosregion_col_ . %4
  str del %_temp 1 18
  set %_temp #strres
  }
return

sub redrawSOSRegions
 gosub redrawSOSRegions_ , %regionDrawRoutine
return

sub redrawSOSRegions_Normal
 menu font align left
 menu font style
 menu font size 8
 menu font color $dddddd
 menu font name lucida console
 menu font BGColor %MapWaterColor
 menu delete SOSRegions
 for %_ 1 %_sosregion_cnt
  {
  set %1 %_sosregion_x1_ . %_ / 16 - 3
  set %2 %_sosregion_y1_ . %_ / 16 - 3
  set %3 %_sosregion_x2_ . %_ / 16 - %1 + 6
  set %4 %_sosregion_y2_ . %_ / 16 - %2 + 6
  set %5 Region . %_
  menu shape %5 %1 %2 %3 %4 3 7 1 %MapRegionColor 2 0
  set %1 %1 + 5 ; draw region # with "border"
  set %2 %2 + 4
  set %3 %1 + 2
  set %4 %2 + 2
  menu font color black
  for %5 %1 %3
  {
  for %6 %2 %4
   {
   menu text SOSRegions %5 %6 %_
   }
  }
 set %1 %1 + 1
 set %2 %2 + 1
 menu font color %MapRegionTColor
 menu text SOSRegions %1 %2 %_
 }
return

sub redrawSOSRegions_Alexandria
 menu font align left
 menu font style
 menu font size 8
 menu font color $dddddd
 menu font name lucida console
 menu delete SOSRegions
 for %_ 1 %_sosregion_cnt
  {
  set %1 %_sosregion_x1_ . %_ / 16
  set %2 %_sosregion_y1_ . %_ / 16
  set %3 %_sosregion_x2_ . %_ / 16 - %1 + 1
  set %4 %_sosregion_y2_ . %_ / 16 - %2 + 1
  menu shape SOSRegions , #systime %1 %2 %3 %4 3 7 1 %MapRegionColor 2 0
  set %1 %1 + 5 ; draw region # with "border"
  set %2 %2 + 4
  set %3 %1 + 2
  set %4 %2 + 2
  menu font color black
  for %5 %1 %3
   {
   for %6 %2 %4
    {
    menu text SOSRegions %5 %6 %_
    }
   }
  set %1 %1 + 1
  set %2 %2 + 1
  menu font color %MapRegionTColor
  menu text SOSRegions %1 %2 %_
  }
return

sub contpos
 if #contposx = %1 && #contposy = %2
  return
 set %5 #cursorx
 set %6 #cursory
 if %0 = 2
  {
  set %3 50
  set %4 20
  }
 set %1 %1 + %3
 set %2 %2 + %4
 set %3 #contposx + %3
 set %4 #contposy + %4
 click %3 %4 g f mc
 for !_ 1 1
  {
  click %1 %2 n f mc
  wait 3
  if #cursorx <> %1 || #cursory <> %2
   set !_ 0
  }
 click %1 %2 p f mc
 click %5 %6 n f mc
return

sub getSOSInfo
 if %shardType <> OSI
  {
  if %shardtype = Odessa
   {
   set !xorigin %1 + 112
   set !yorigin %2 + 51
   set !xpos %1 + 102
   set !ypos %2 + 59
   ;set !ycheck %2 + 245
   }
  else
   {
   set !xorigin %1 + 35
   set !yorigin %2 + 240
   set !xpos %1 + 71
   set !ypos %2 + 270
   ;set !ycheck %2 + 245
   }
  set !xextent !xorigin + 150
  set !sos_coords
  savepix !xpos !ypos 1
  set !SOSTextColor #pixcol
  while !xorigin < !xextent
   {
   if _ , %ShardType , _ in _Pandora_Odyssey_
    gosub getchar !xorigin !yorigin = !SOSTextColor number
   else
    gosub readchar !xorigin !yorigin = !SOSTextColor ; number
   if #result <> #spc 1
    set !sos_coords !sos_coords , #result
   set !xorigin !xorigin + %1
   }
  }
 else
  {
;  if %shardtype = Pandora
;   {
;   ; coordinate text: x offset 30 y offset 240
;   }

; store gumps position

  set !contposx %1
  set !contposy %2

; get number of lines on gump

  set !offsetx !contposx + 27
  set !offsety !contposy + 50
  for !linecnt 1 20
   {
   gosub GetChar !offsetx !offsety = 524288 number
   if %2 = #spc
    {
    set !temp !offsetx + 8
    gosub GetChar !temp !offsety = 524288 number
    }
   if %2 = #spc
    {
    set !linecnt !linecnt - 1
    break
    }
   set !offsety !offsety + 18
   }

; load text from gump

  set !gumptext
  set !labeltext
  set !startline 1 ;!linecnt - 2
  set !offsetx !contposx + 27
  set !offsety !contposy + 32 + !startline * 18
  for !lineidx !startline !linecnt
   {
   gosub GetText !offsetx !offsety = 524288 number 200
   set !gumptext !gumptext , #strres , #spc
   set !labeltext !labeltext , #strres , $
   set !offsety !offsety + 18
   }

  ;menu delete EUOLabel9
  ;menu Text EUOLabel9 328 118 !labeltext

; extract coordinates from gumptext

  str len !gumptext
  set !textlen #strres
  for !startpos 1 !textlen
   {
   ; for all coords not starting with 0
   str mid !gumptext !startpos 1
   str pos 123456789 #strres
   if #strres > 0
    break
   ; for coords starting with 0
   str mid !gumptext !startpos 3
   if #strres = 0o , #spc
    {
    set !temp !startpos + 3
    str mid !gumptext !temp 1
    str pos 0123456789 #strres
    if #strres > 0
     break
    }
   }
  for !endpos !startpos !textlen
   {
   str mid !gumptext !endpos 1
   set #strres #strres , #spc
   if #strres = W , #spc || #strres = E , #spc
    {
    set !endpos !endpos + 1
    break
    }
   }
  set !endpos !endpos - !startpos
  str mid !gumptext !startpos !endpos
  set !sos_coords #strres
  }

 set %1 !sos_coords

 str pos %1 o
 set #strres #strres - 1
 str left %1 #strres
 set !sos_worldy #strres * 60
 str pos %1 o
 str del %1 1 #strres
 set %1 #strres
 str pos %1 '
 set #strres #strres - 1
 str left %1 #strres
 set !sos_worldy !sos_worldy + #strres
 str pos %1 ,
 str del %1 1 #strres
 set %1 #strres
 str pos %1 o
 set #strres #strres - 1
 str left %1 #strres
 set !sos_worldx #strres * 60
 str pos %1 o
 str del %1 1 #strres
 set %1 #strres
 str pos %1 '
 set #strres #strres - 1
 str left %1 #strres
 set !sos_worldx !sos_worldx + #strres
 set !sos_worldy !sos_worldy , 0000000 / 52734375
 if n in !sos_coords 3
  set !sos_worldy 1624 - !sos_worldy
 if !sos_worldy < 0
  set !sos_worldy !sos_worldy + 4096
 if s in !sos_coords 3
  set !sos_worldy 1624 + !sos_worldy
 if !sos_worldy >= 4096
  set !sos_worldy !sos_worldy - 4096
 set !sos_worldx !sos_worldx , 00000 / 421875
 if w in !sos_coords 3
  set !sos_worldx 1323 - !sos_worldx
 if !sos_worldx < 0
  set !sos_worldx !sos_worldx + 5120
 if e in !sos_coords 3
  set !sos_worldx 1323 + !sos_worldx
 if !sos_worldx >= 5120
  set !sos_worldx !sos_worldx - 5120

;setuotitle #lobjectid !sos_worldx !sos_worldy !sos_coords
return

