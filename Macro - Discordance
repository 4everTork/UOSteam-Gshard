      pause 500
      useskill 'Discordance'
      waitfortarget 2000
      target! 'discord0'
      pause 500
    endif
  endwhile
endif
clearjournal
useskill 'Hiding'
while not @injournal 'voce ficou invisivel'
  if @injournal 'nao conseguiu'
    useskill 'Hiding'
    pause 500
  endif
endwhile
pause 20000
