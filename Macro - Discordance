//////
// Macro de Discordance
// Ter 10 animais ao seu redor.
//////
// Selecionar os animais
if not @findobject 'discord1'
  headmsg '*Selecione o primeiro animal.*'
  promptalias 'discord1'
endif
if not @findobject 'discord2'
  headmsg '*Selecione o segundo animal.*'
  promptalias 'discord2'
endif
if not @findobject 'discord3'
  headmsg '*Selecione o terceiro animal.*'
  promptalias 'discord3'
endif
if not @findobject 'discord4'
  headmsg '*Selecione o quarto animal.*'
  promptalias 'discord4'
endif
if not @findobject 'discord5'
  headmsg '*Selecione o quinto animal ou aperte ESC.*'
  promptalias 'discord5'
endif
if not @findobject 'discord6'
  headmsg '*Selecione o sexto animal ou aperte ESC.*'
  promptalias 'discord6'
endif
if not @findobject 'discord7'
  headmsg '*Selecione o setimo animal ou aperte ESC.*'
  promptalias 'discord7'
endif
if not @findobject 'discord8'
  headmsg '*Selecione o oitavo animal ou aperte ESC.*'
  promptalias 'discord8'
endif
if not @findobject 'discord9'
  headmsg '*Selecione o oitavo animal ou aperte ESC.*'
  promptalias 'discord9'
endif
if not @findobject 'discord0'
  headmsg '*Selecione o oitavo animal ou aperte ESC.*'
  promptalias 'discord0'
endif
if not @findobject 'discord1'
  unsetalias 'discord1'
endif
if not @findobject 'discord2'
  unsetalias 'discord2'
endif
if not @findobject 'discord3'
  unsetalias 'discord3'
endif
if not @findobject 'discord4'
  unsetalias 'discord4'
endif
if not @findobject 'discord5'
  unsetalias 'discord5'
endif
if not @findobject 'discord6'
  unsetalias 'discord6'
endif
if not @findobject 'discord7'
  unsetalias 'discord7'
endif
if not @findobject 'discord8'
  unsetalias 'discord8'
endif
if not @findobject 'discord9'
  unsetalias 'discord9'
endif
if not @findobject 'discord0'
  unsetalias 'discord0'
endif
// Discordance 1
if @findalias 'discord1'
  @clearjournal
  useskill 'Discordance'
  waitfortarget 2000
  target! 'discord1'
  pause 500
  while not @injournal 'reduzindo a forca' 'system'
    if @injournal 'ja esta em' 'system'
      break
    endif
    if @injournal 'nao conseguiu' 'system'
      pause 500
      useskill 'Discordance'
      waitfortarget 2000
      target! 'discord1'
      pause 500
    endif
  endwhile
endif
// Discordance 2
if @findalias 'discord2'
  @clearjournal
  useskill 'Discordance'
  waitfortarget 2000
  target! 'discord2'
  pause 500
  while not @injournal 'reduzindo a forca' 'system'
    if @injournal 'ja esta em' 'system'
      break
    endif
    if @injournal 'nao conseguiu' 'system'
      pause 500
      useskill 'Discordance'
      waitfortarget 2000
      target! 'discord2'
      pause 500
    endif
  endwhile
endif
// Discordance 3
